{
    "version": "0.1.0",
    "name": "solvote",
    "instructions": [
        {
            "name": "initApp",
            "accounts": [
                {
                    "name": "app",
                    "isMut": true,
                    "isSigner": false
                },
                {
                    "name": "signer",
                    "isMut": true,
                    "isSigner": true
                },
                {
                    "name": "systemProgram",
                    "isMut": false,
                    "isSigner": false
                }
            ],
            "args": []
        },
        {
            "name": "createSpace",
            "accounts": [
                {
                    "name": "app",
                    "isMut": true,
                    "isSigner": false
                },
                {
                    "name": "communitySpace",
                    "isMut": true,
                    "isSigner": false
                },
                {
                    "name": "signer",
                    "isMut": true,
                    "isSigner": true
                },
                {
                    "name": "systemProgram",
                    "isMut": false,
                    "isSigner": false
                }
            ],
            "args": [
                {
                    "name": "name",
                    "type": "string"
                }
            ]
        },
        {
            "name": "createProposal",
            "accounts": [
                {
                    "name": "communitySpace",
                    "isMut": true,
                    "isSigner": false
                },
                {
                    "name": "proposal",
                    "isMut": true,
                    "isSigner": false
                },
                {
                    "name": "signer",
                    "isMut": true,
                    "isSigner": true
                },
                {
                    "name": "systemProgram",
                    "isMut": false,
                    "isSigner": false
                }
            ],
            "args": [
                {
                    "name": "spaceKey",
                    "type": "publicKey"
                },
                {
                    "name": "title",
                    "type": "string"
                },
                {
                    "name": "description",
                    "type": "string"
                },
                {
                    "name": "options",
                    "type": {
                        "vec": "string"
                    }
                },
                {
                    "name": "deadline",
                    "type": "u64"
                }
            ]
        },
        {
            "name": "castVote",
            "accounts": [
                {
                    "name": "proposal",
                    "isMut": true,
                    "isSigner": false
                },
                {
                    "name": "vote",
                    "isMut": true,
                    "isSigner": false
                },
                {
                    "name": "signer",
                    "isMut": true,
                    "isSigner": true
                },
                {
                    "name": "systemProgram",
                    "isMut": false,
                    "isSigner": false
                }
            ],
            "args": [
                {
                    "name": "options",
                    "type": {
                        "vec": {
                            "defined": "HasVotedFor"
                        }
                    }
                }
            ]
        }
    ],
    "accounts": [
        {
            "name": "App",
            "type": {
                "kind": "struct",
                "fields": [
                    {
                        "name": "spaceCount",
                        "type": "u32"
                    }
                ]
            }
        },
        {
            "name": "Proposal",
            "type": {
                "kind": "struct",
                "fields": [
                    {
                        "name": "spaceKey",
                        "type": "publicKey"
                    },
                    {
                        "name": "title",
                        "type": "string"
                    },
                    {
                        "name": "description",
                        "type": "string"
                    },
                    {
                        "name": "options",
                        "type": {
                            "vec": {
                                "defined": "VoteOption"
                            }
                        }
                    },
                    {
                        "name": "deadline",
                        "type": "u64"
                    }
                ]
            }
        },
        {
            "name": "CommunitySpace",
            "type": {
                "kind": "struct",
                "fields": [
                    {
                        "name": "name",
                        "type": "string"
                    },
                    {
                        "name": "proposalCount",
                        "type": "u32"
                    },
                    {
                        "name": "usersCount",
                        "type": "u32"
                    }
                ]
            }
        },
        {
            "name": "Vote",
            "type": {
                "kind": "struct",
                "fields": [
                    {
                        "name": "proposal",
                        "type": "publicKey"
                    },
                    {
                        "name": "user",
                        "type": "publicKey"
                    },
                    {
                        "name": "hasVotedFor",
                        "type": {
                            "vec": {
                                "defined": "HasVotedFor"
                            }
                        }
                    },
                    {
                        "name": "hasVoted",
                        "type": "bool"
                    }
                ]
            }
        }
    ],
    "types": [
        {
            "name": "VoteOption",
            "type": {
                "kind": "struct",
                "fields": [
                    {
                        "name": "label",
                        "type": "string"
                    },
                    {
                        "name": "count",
                        "type": "u32"
                    }
                ]
            }
        },
        {
            "name": "HasVotedFor",
            "type": {
                "kind": "struct",
                "fields": [
                    {
                        "name": "index",
                        "type": "u8"
                    },
                    {
                        "name": "ratio",
                        "type": "u16"
                    }
                ]
            }
        }
    ],
    "errors": [
        {
            "code": 6000,
            "name": "ProposalIsOver",
            "msg": "Proposal is closed"
        },
        {
            "code": 6001,
            "name": "ChoiceIndexOutOfScope",
            "msg": "Choice index invalid"
        },
        {
            "code": 6002,
            "name": "AlreadyVoted",
            "msg": "Already voted"
        },
        {
            "code": 6003,
            "name": "VoteRatioExceeded",
            "msg": "Vote ratio exceeded"
        },
        {
            "code": 6004,
            "name": "ProposalOptionsExceeded",
            "msg": "Too many options"
        }
    ]
}